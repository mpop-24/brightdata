'use strict';const brd_hs_popup={init(){this.open_popup_by_url();this.open_popup_by_data_attr();this.open_popup_by_post_message();this.prevent_url_change();this.exit_popup();this.body_class();this.init_play_buttons()},open_popup_by_data_attr(){jQuery(window).on('elementor/frontend/init',()=>{elementorFrontend.on('components:init',function(){jQuery(document.body).on('click','[data-popup_id]',function(){let popup_id=jQuery(this).attr('data-popup_id');elementorFrontend.documentsManager.documents[popup_id].showModal()})})})},open_popup_by_url(){const query_string=window.location.search;const url_params=new URLSearchParams(query_string);let hs_signup=url_params.get('hs_signup');hs_signup=hs_signup?parseInt(hs_signup):!1;const datasets_meeting=url_params.get('datasets_meeting')=='1';hs_signup=datasets_meeting&&!hs_signup?datasets_meeting:hs_signup;let popup_id=brd_data.hs_signup_id;if(hs_signup=='1'){jQuery(window).on('elementor/frontend/init',()=>{elementorFrontend.on('components:init',function(){elementorFrontend.documentsManager.documents[popup_id].showModal()})})}else if(hs_signup){popup_id=parseInt(hs_signup);jQuery(window).on('elementor/frontend/init',()=>{elementorFrontend.on('components:init',()=>{setTimeout(function(){elementorFrontend.documentsManager.documents[popup_id].showModal()},1000)})})}},open_popup_by_post_message(){jQuery(window).on('elementor/frontend/init',()=>{elementorFrontend.on('components:init',()=>{window.addEventListener('message',function(e){let popup_id=brd_data.hs_signup_id;if(e.data!='open_wpsignup_form'||e.origin!=window.location.origin){return}
setTimeout(function(){elementorFrontend.documentsManager.documents[popup_id].showModal()},1000)})})})},prevent_url_change(){const popup_triggers=['#','#contact-sales','#request-dataset'];for(let i=0;i<popup_triggers.length;i++){const trg=jQuery(`a[href="${popup_triggers[i]}"]`);if(!trg||!trg.length)
continue;for(let k=0;k<trg.length;k++)
trg[k].addEventListener('click',e=>e.preventDefault());}},exit_popup(){const popups=[116405,121278];if(!popups)
return;popups.forEach(popup=>{jQuery(document).on('elementor/popup/show',(event,id,ins)=>{if(ins.$element[0].classList.contains('elementor-'+popup)){ins.$element[0].querySelector('#close_btn').addEventListener('click',()=>{elementorFrontend.documentsManager.documents[popup].getModal().hide()})}})})},body_class(){jQuery(document).on('elementor/popup/show',(event,id,instance)=>{jQuery('body').addClass('dialog-open')});jQuery(document).on('elementor/popup/hide',(event,id,instance)=>{jQuery('body').removeClass('dialog-open');setTimeout(()=>{if(jQuery('.brd-popup-calendly').length){jQuery('.brd-popup-calendly').removeClass('brd-popup-calendly')}},1000)})},init_play_buttons(){jQuery(document).ready(function($){if($('.brd_btn #play_button, .brd_btn .play_button')){$('.brd_btn #play_button, .brd_btn .play_button').on('click',function(e){e.preventDefault();let video_link=$(this).attr('href');if(!video_link)
return;elementorFrontend.utils.lightbox.showModal({url:`${video_link}`,videoAspectRatio:'169',type:'video',entranceAnimation:'bounceInDown',})})}})}};brd_hs_popup.init();'use strict';const brd_elementor_accordion={init(){this.arrows()},arrows(){jQuery(window).on('elementor/frontend/init',()=>{const icons=document.querySelectorAll('.elementor-accordion-icon');icons.forEach(ic=>{const spans=ic.querySelectorAll('span');const svg=document.createElementNS('http://www.w3.org/2000/svg','svg');const use=document.createElementNS('http://www.w3.org/2000/svg','use');svg.setAttribute('aria-hidden','true');use.setAttributeNS('http://www.w3.org/1999/xlink','href','#ic_dropdown');svg.classList.add(`ic`);spans.forEach(sp=>{const i=sp.querySelector('i');let stage;if(sp.classList.contains('elementor-accordion-icon-opened')){stage='opened'}else if(sp.classList.contains('elementor-accordion-icon-closed')){stage='closed'}
if(!stage)
return;svg.classList.add(`ic_${stage}`);const directions=['up','right','down','left'];for(let dir of directions){if(i.classList.contains(`fa-chevron-${dir}`))
svg.classList.add(`ic_${stage}_${dir}`)}
sp.remove()});svg.appendChild(use);ic.appendChild(svg)})})},};brd_elementor_accordion.init()