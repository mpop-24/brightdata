'use strict';(function($){function get_data(){let section=document.querySelector('section.scraper_directory');if(!section)
return brd_com_dir_data;let data=section.getAttribute('data-com_dir');try{return JSON.parse(data)}catch(e){return brd_com_dir_data}}
$('.company_search input').autocomplete({open:function(event,ui){jQuery(event.target).addClass('active')},close:function(event,ui){jQuery(event.target).removeClass('active')},source:get_data(),appendTo:'.scraper_directory .ui-autocomplete',select:function(event,ui){let event_data={'event':'ide-search','search-query':ui.item.label};window.dataLayer.push(event_data);setTimeout(function(){window.location.href=ui.item.url},200)},}).autocomplete('widget').css('position','fixed').css('top','0').css('z-index','1');$(document.body).on('click','.company_header__nav_item:not(.back) a',e=>{let target=$(e.target);if(target.closest('.company_header__nav_item.active').length!=0)
return;$('.company_header__nav_item.active').removeClass('active');target.closest('.company_header__nav_item').addClass('active')});$(document).ready(function(){if($('body').hasClass('search-no-results')||location.hash){$('body, html').animate({scrollTop:$('#company_search').offset().top-180},100);if(location.hash){let hash=location.hash.replace('#','');$('.scroll_btn[data-content="'+hash+'"]').click()}}})})(jQuery)