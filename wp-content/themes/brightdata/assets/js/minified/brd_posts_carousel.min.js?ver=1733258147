'use strict';const brd_events_slider={init(){this.swiper();this.play_popup()},play_popup(){document.querySelectorAll('.btn_play').forEach(btn=>{btn.addEventListener('click',e=>{const url=e.target.getAttribute('data-url');elementorFrontend.documentsManager.documents[8238].showModal();const popup=jQuery('#elementor-popup-modal-8238 '+'.elementor-8238 .elementor-widget-wrap');const iframe=document.createElement('iframe');iframe.setAttribute('src',url);iframe.setAttribute('width','640');iframe.setAttribute('height','360');iframe.setAttribute('title','YouTube video player');iframe.setAttribute('frameborder','0');iframe.setAttribute('allow','accelerometer; '+'autoplay; clipboard-write; '+'encrypted-media; gyroscope; '+'picture-in-picture');popup.append(iframe)})})},swiper(){const swiper1_sel='#wp-widget-posts_slider_3.swiper-container';new Swiper(swiper1_sel,{slidesPerView:1,spaceBetween:20,resistanceRatio:0.5,breakpoints:{576:{slidesPerView:2,spaceBetween:30,},},pagination:{el:'#wp-widget-posts_slider_3 .swiper-pagination',},navigation:{nextEl:'#wp-widget-posts_slider_3 + .swiper-buttons '+'.swiper-button-next',prevEl:'#wp-widget-posts_slider_3 + .swiper-buttons '+'.swiper-button-prev',},});const swiper2_sel='#wp-widget-posts_slider_2.swiper-container';new Swiper(swiper2_sel,{slidesPerView:1,spaceBetween:20,resistanceRatio:0.5,breakpoints:{576:{slidesPerView:2,spaceBetween:30,},1024:{slidesPerView:3,spaceBetween:30,}},pagination:{el:'#wp-widget-posts_slider_2 .swiper-pagination',},navigation:{nextEl:'#wp-widget-posts_slider_2 + .swiper-buttons '+'.swiper-button-next',prevEl:'#wp-widget-posts_slider_2 + .swiper-buttons '+'.swiper-button-prev',},});const swiper3_sel='#wp-widget-posts_slider_0.swiper-container';new Swiper(swiper3_sel,{slidesPerView:1,spaceBetween:20,resistanceRatio:0.5,breakpoints:{576:{slidesPerView:1,spaceBetween:30,},1024:{slidesPerView:4,spaceBetween:30,}},pagination:{el:'#wp-widget-posts_slider_0 .swiper-pagination',},navigation:{nextEl:'#wp-widget-posts_slider_0 + .swiper-buttons '+'.swiper-button-next',prevEl:'#wp-widget-posts_slider_0 + .swiper-buttons '+'.swiper-button-prev',},});const swiper4_sel='#wp-widget-posts_slider_5.swiper-container';new Swiper(swiper4_sel,{slidesPerView:1,spaceBetween:20,resistanceRatio:0.5,breakpoints:{576:{slidesPerView:1,spaceBetween:30,},1024:{slidesPerView:4,spaceBetween:30,}},pagination:{el:'#wp-widget-posts_slider_5 .swiper-pagination',},navigation:{nextEl:'#wp-widget-posts_slider_5 + .swiper-buttons '+'.swiper-button-next',prevEl:'#wp-widget-posts_slider_5 + .swiper-buttons '+'.swiper-button-prev',},});jQuery('.brd_pc_wrapper').each(function(){let data_json=jQuery(this).attr('data-swipper_attrs');let data=JSON.parse(data_json);let margin=typeof data.margin!=='undefined'?data.margin:30;let margin_tablet=typeof data.margin_tablet!=='undefined'?data.margin_tablet:30;let margin_mobile=typeof data.margin_mobile!=='undefined'?data.margin_mobile:20;let auto_height=data.autoHeight;new Swiper(this,{slidesPerView:data.slidesPerView_mobile,slidesPerColumn:data.slidesPerColumn_mobile,spaceBetween:margin_mobile,resistanceRatio:0.5,autoHeight:auto_height,breakpoints:{576:{slidesPerView:data.slidesPerView_tablet,spaceBetween:margin_tablet,slidesPerColumn:data.slidesPerColumn_tablet,autoHeight:auto_height,},1024:{slidesPerView:data.slidesPerView,spaceBetween:margin,slidesPerColumn:data.slidesPerColumn,autoHeight:auto_height,}},pagination:{el:jQuery(this).parent().find('.swiper-pagination')[0],clickable:!0,},navigation:{nextEl:jQuery(this).parent().find('.swiper-button-next')[0],prevEl:jQuery(this).parent().find('.swiper-button-prev')[0],},})})},};jQuery(window).on('elementor/frontend/init',function(){if('undefined'===typeof Swiper){elementorFrontend.hooks.addAction('frontend/element_ready/global',$scope=>{if($scope.hasClass('elementor-widget-brd_post_carousel')){elementorFrontend.utils.assetsLoader.load('script','swiper').then(()=>{brd_events_slider.init()})}})}else{brd_events_slider.init()}})