'use strict';const brd_proxy_manager={init(){this.accordion()},accordion(){let btns=document.querySelectorAll('.proxy_install_btn');btns.forEach(function(btn){btn.addEventListener('click',function(event){event.preventDefault();let item=this;let selected=item.getAttribute('data-class');let boxes=document.querySelectorAll('.proxy_install_inst');let parent=item.closest('.btn');if(item.classList.contains('btn--brand'))
return;btns.forEach(function(button){let btn_parent=button.closest('.btn');btn_parent.classList.remove('btn--brand');btn_parent.classList.add('btn--transparent','with_borders')});parent.classList.add('btn--brand');parent.classList.remove('btn--transparent','with_borders');boxes.forEach(function(content){if(content.classList.contains(selected))
content.style.display='block';else content.style.display='none'})})})}};brd_proxy_manager.init()