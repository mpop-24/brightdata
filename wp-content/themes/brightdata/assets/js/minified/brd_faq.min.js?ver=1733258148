'use strict';jQuery(document).ready(function($){jQuery(document.body).on('click','.elementor-widget-brd_accordion'+' .elementor-tab-title',function(e){e.preventDefault();e.stopPropagation();let title=$(e.target).closest('.elementor-accordion-item').find('.elementor-tab-title');let content=$(e.target).closest('.elementor-accordion-item').find('.elementor-tab-content');let is_active=title.hasClass('elementor-active');if(!is_active){$('.elementor-widget-brd_accordion '+'.elementor-tab-title.elementor-active').removeClass('elementor-active');$('.elementor-widget-brd_accordion '+'.elementor-tab-content.elementor-active').removeClass('elementor-active').slideUp(350);content.slideDown(350).addClass('elementor-active');title.addClass('elementor-active')}else{content.hide().removeClass('elementor-active');title.removeClass('elementor-active')}
return!1});jQuery(document.body).on('click','a.elementor-accordion-title',function(e){e.preventDefault();e.stopPropagation();$(e.target).closest('.elementor-accordion-item').find('.elementor-tab-title').click()})})