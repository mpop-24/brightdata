'use strict';jQuery(document).ready(function($){if(brd_body.querySelector('.scroll_btn')){$('.letters_list a').click(function(event){if(!$('body').hasClass('archive'))
return;event.preventDefault();var companies_box=$('.directories_footer .company_details'+' #company_search .company_box');var url=$(this).attr('href');var hash=url.split('#')[1];var target_elm=$('.company_box__title#'+hash);var target_elm_offset=target_elm?target_elm[0].offsetTop:0;companies_box.animate({scrollTop:target_elm_offset-25},1250)})}})