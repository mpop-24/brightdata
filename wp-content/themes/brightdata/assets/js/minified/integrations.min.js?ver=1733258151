'use strict';const brd_header_dark={init(){this.listeners()},listeners(){const _this=this;document.addEventListener('set_header_sticky',_this.unset_dark);document.addEventListener('unset_header_sticky',_this.set_dark)},unset_dark(){const is_dark=document.body.classList.contains('dark_header');const is_dark_to_light=document.body.classList.contains('header_dark_to_light');if(is_dark&&is_dark_to_light)
document.body.classList.remove('dark_header')},set_dark(){const is_dark=document.body.classList.contains('dark_header');const is_dark_to_light=document.body.classList.contains('header_dark_to_light');if(!is_dark&&is_dark_to_light)
document.body.classList.add('dark_header')},};brd_header_dark.init();'use strict';const brd_fire_search_event={init(){this.fire_search_event()},fire_search_event(){let current_url=window.location.href;let url_object=new URL(current_url);let pathname=url_object.pathname;pathname=pathname.replace(/^\//,'');let segments=pathname.split('/');let page_name=segments[0];let search_form=document.querySelector('#blog_search');let search_query=document.querySelector('#blog_search #q');if(!search_query||!search_form)
return;let search_btn=document.querySelector('#blog_search button');search_btn.addEventListener('click',function(event){event.preventDefault();let event_data={'event':page_name+'-search','search-query':search_query.value};window.dataLayer.push(event_data);search_form.submit()})},};document.addEventListener('DOMContentLoaded',function(){brd_fire_search_event.init()})