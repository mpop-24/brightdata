'use strict';const brd_line_nested_code_tabs={init(){if(typeof window.Prism==='undefined')
return;this.listeners()},highlight_active_tabs(){const containers=document.querySelectorAll('.line_item_tabs_panel_shown');if(!containers.length)
return;for(let i=0;i<containers.length;i++)
window.Prism.highlightAllUnder(containers[i]);},highlight_activated_tab(e){const container=e.currentTarget.nextElementSibling;if(!container)
return;window.Prism.highlightAllUnder(container,!1,()=>{brd_line_nested_code_tabs.scroll_to_highlighted(container)})},scroll_to_highlighted(container){const scroll=container.dataset.scroll;if(!scroll)
return;const target=container.querySelector('.line-highlight');const wrapper=container.querySelector('.tabs_code__code_wrap');if(!wrapper||!target)
return;const offset=target.offsetTop-(wrapper.offsetHeight-target.offsetHeight)/2;wrapper.scrollTop=offset>target.offsetTop?target.offsetTop-44:offset},listeners(){const tabs=document.querySelectorAll(`.tabs_code_item, `+`.line_nested_code_tabs__tabs_item, `+`.line_nested_code_tabs__tabs_nested_item`);for(let i=0;i<tabs.length;i++)
tabs[i].addEventListener('click',this.highlight_activated_tab);window.addEventListener('resize',this.highlight_active_tabs)},};brd_line_nested_code_tabs.init()